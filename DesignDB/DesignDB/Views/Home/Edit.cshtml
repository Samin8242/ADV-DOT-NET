
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
<form method="post">
    <input type="hidden" name="Id" value="@Model.Id" />
    Name:  <input type="text" name="Name" class="form-control" value="@Model.Name" />
    CId:  <input type="text" name="CId" class="form-control" value="@Model.CId" />
    Price:  <input type="text" name="Price" class="form-control" value="@Model.Price" />
    Type:  <input type="text" name="Type" class="form-control" value="@Model.Type" />
    <input type="submit" value="Update" class="btn btn-success" />


</form>
